#include <stdio.h>

int main() {
    const int arraySize = 5;
    int array[arraySize] = {10, 20, 30, 40, 50};

    int sum = 0;
    for (int i = 0; i < arraySize; i += 2) {
        sum += array[i];
    }

    printf("The sum of every alternate element starting from index 0 is: %d\n", sum);

    return 0;
}
